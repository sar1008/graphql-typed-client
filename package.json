    "roots": [
      "<rootDir>/src"
    ],
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!**/node_modules/**",
      "!src/_old/**",
      "!**/*.case.ts",
      "!src/testHelpers/**/*.ts"
    ],
    "coverageReporters": [
      "text"
    ]
