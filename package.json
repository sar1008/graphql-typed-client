  "devDependencies": {
    "@types/common-tags": "^1.8.0",
    "@types/jest": "^23.3.13",
    "@types/listr": "^0.13.0",
    "@types/mkdirp": "^0.5.2",
    "@types/prettier": "^1.16.0",
    "@types/rimraf": "^2.0.2",
    "@types/ws": "^6.0.1",
    "common-tags": "^1.8.0",
    "husky": "^1.3.1",
    "jest": ">=22 <24",
    "pretty-quick": "^1.10.0",
    "ts-jest": "^23.10.5",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "@types/graphql": "^14.0.5",
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/node": "^10.12.24",
    "@types/qs": "^6.5.1",
    "chalk": "^2.4.2",
    "commander": "^2.19.0",
    "graphql": "^14.1.1",
    "isomorphic-fetch": "^2.2.1",
